{
  "name": "se495",
  "version": "1.0.0",
  "description": "Web scraping project",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon",
    "migration:up": "npx db-migrate up",
    "migration:down": "npx db-migrate down",
    "migration:create": "npx db-migrate create $NAME --sql-file",
    "build:apidoc": "./node_modules/apidoc/bin/apidoc -i src -o dist/docs/apis",
    "test": "jest --runInBand --detectOpenHandles",
    "pretest": "npm run test:db:up",
    "test:db:up": "docker-compose up -d && sleep 5 && export DATABASE_NAME=mednet; export DATABASE_USER=mednetadmin; export DATABASE_PASS=mednetpass; npm run migration:up",
    "posttest": "docker-compose down"
  },
  "author": "Milos Caranovic",
  "license": "ISC",
  "dependencies": {
    "@swc/wasm": "^1.2.93",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^15.6.1",
    "@types/puppeteer": "^5.4.3",
    "@types/validator": "^13.6.3",
    "apidoc": "^0.29.0",
    "auto-loader": "^0.2.0",
    "bcrypt": "^5.0.1",
    "bufferutil": "^4.0.4",
    "canvas": "^2.8.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "db-migrate-mysql": "^2.1.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "html-entities": "^2.3.2",
    "i18n": "^0.13.3",
    "i18next-fs-backend": "^1.1.4",
    "i18next-http-backend": "^1.3.1",
    "i18next-http-middleware": "^3.1.4",
    "joi": "^17.4.1",
    "joi-cron-expression": "^1.0.1",
    "joi-date-extensions": "^1.2.0",
    "joi-phone-number": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "neverthrow": "^4.2.1",
    "node-notifier": "^10.0.0",
    "nodemon": "^2.0.9",
    "puppeteer": "^13.0.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.5",
    "sequelize-typescript": "^2.1.0",
